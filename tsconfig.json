        "target": "ES5",
        "module": "CommonJS",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "outDir": "./src/build"
    },
    "include": ["./src/*"],
    "exclude": ["./src/interfaces"]
}
